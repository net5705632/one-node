{
"log": {
"loglevel": "warning"
},
"inbounds": [
{
"listen": null,
"port": $PORT,
"protocol": "vless",
"settings": {
"decryption": "none",
"clients": [
{
"email": "idx-tcp",
"flow": "",
"id": "$UUID"
}
]
},
"streamSettings": {
"network": "h2",
"security": "tls",
"httpSettings": {
"host": ["your-domain.com"],
"path": "/your-path"
},
"tlsSettings": {
"certificates": [
{
"certificateFile": "/path/to/cert.crt",
"keyFile": "/path/to/private.key"
}
]
}
}
}
],
"outbounds": [
{
"protocol": "freedom"
}
]
}
